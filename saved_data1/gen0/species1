digraph finite_state_machine {
	rankdir=BT size="8,5"
	node [shape=circle]
	subgraph cluster_input {
		node [color=white style=filled]
		color=lightgrey style=filled
		1
		2
		3
		4
		5
		6
		7
		label=input
	}
	subgraph cluster_output {
		node [color=white style=filled]
		color=lightgrey style=filled
		8
		label=output
	}
	1 -> 8 [label=0.12]
	2 -> 8 [label=-0.48]
	3 -> 8 [label=1.03]
	4 -> 8 [label=1.15]
	5 -> 8 [label=2.11]
	6 -> 8 [label=-0.36]
	7 -> 8 [label=-0.14]
}
